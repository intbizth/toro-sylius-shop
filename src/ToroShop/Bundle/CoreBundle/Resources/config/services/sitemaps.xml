<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="gdd_core.sitemap_provider.cms_url_provider" class="ToroShop\Bundle\CoreBundle\Sitemap\Provider\CmsUrlProvider"
                 public="false">
            <argument type="service" id="bitbag_sylius_cms_plugin.repository.page"/>
            <argument type="service" id="router"/>
            <argument type="service" id="sylius.sitemap_url_factory"/>
            <argument type="service" id="sylius.context.locale"/>
            <tag name="sylius.sitemap_provider"/>
        </service>
    </services>
</container>
