<?xml version="1.0" encoding="UTF-8"?>


<constraint-mapping xmlns="http://symfony.com/schema/dic/constraint-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/constraint-mapping http://symfony.com/schema/dic/services/constraint-mapping-1.0.xsd">
    <class name="ToroShop\Bundle\AddressBundle\Model\GeoNameTranslation">
        <property name="slug">
            <constraint name="NotBlank">
                <option name="message">toro.address.geo_name_translation.slug.not_blank</option>
                <option name="groups">toro</option>
            </constraint>
        </property>
        <property name="name">
            <constraint name="NotBlank">
                <option name="message">toro.address.geo_name_translation.name.not_blank</option>
                <option name="groups">toro</option>
            </constraint>
        </property>
        <property name="geoName">
            <constraint name="NotBlank">
                <option name="message">toro.address.geo_name_translation.geo_mame.not_blank</option>
                <option name="groups">toro</option>
            </constraint>
        </property>
    </class>
</constraint-mapping>
